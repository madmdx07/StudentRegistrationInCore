                @Html.DisplayFor(modelItem => item.Cricket)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Chess)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tennis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Drawing)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Class.ClassId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doc.DocId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender.GenderId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Image.ImageId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
